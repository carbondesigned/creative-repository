<style>
  /* Default to one column on small screens */
  .masonry-grid {
    column-count: 1;
    column-gap: 1rem;
  }

  /* On medium screens (md: breakpoint at 768px by default), use 2 columns */
  @media (min-width: 768px) {
    .masonry-grid {
      column-count: 2;
    }
  }

  /* On large screens (lg: breakpoint at 1024px by default), use 3 columns */
  @media (min-width: 1024px) {
    .masonry-grid {
      column-count: 3;
    }
  }

  .masonry-grid-item {
    break-inside: avoid;
    margin-bottom: 1rem;
  }
</style>
